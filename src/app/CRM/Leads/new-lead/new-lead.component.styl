section 
    display flex
    flex-direction row
    width 980px 
    margin 0 auto
    form
        display flex
        flex-direction row
        justify-content center
        &:nth-of-type(1)
            flex 7
            & div
                &:nth-of-type(1)
                    flex 3
                &:nth-of-type(2)
                    flex 5
        &:nth-of-type(2)
            flex 2
        div.wrapp
            padding  0 16px
